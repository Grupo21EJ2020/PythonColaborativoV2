class Enciclopedia():
    def __init__ ( self, hojas, tipo ):
        self.hojas = hojas
        self.tipo = tipo

    def imprimirinfo(self):
        print(f"Hojas: {self.hojas} tipo: {self.tipo}")

class Tenis():
    def __init__ (self, talla, color):
        self.talla = talla
        self.color = color 

    def imprimirinfo(self):
        print(f"talla: {self.talla} color: {self.color")

class Libro():
    def __init__ (self, tamaño, materia):
        self.tamaño = tamaño
        self.materia = materia 

    def imprimirinfo(self):
        print(f"tamaño: {self.tamaño} materia {self.materia")
