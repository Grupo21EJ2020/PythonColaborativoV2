class Tenis():
    def__init__(self, marca, color):
       self.marca = marca
       self.color = color 
       
    def imprimirinfo(self):
        print(f"Marca: {self.marca} Color: {self.color}") 
        
        
class Carro():
    def__init__(self, compañia, color, año):
       self.compañia = compañia 
       self.color = color
       self.año = año 
       
    def imprimirinfo(self):
        print(f"Compañia: {self.compañia} Color: {self.color} Año: {self.año}")
        
               
Class Celular():
    def__init__(self, precio, color, marca):
       self.precio = precio
       self.color = color
       self.marca = marca 
       
    def imprimirinfo(self):
        print(f"Precio: {self.precio} Color: {self.color} Marca: {self.marca}")
